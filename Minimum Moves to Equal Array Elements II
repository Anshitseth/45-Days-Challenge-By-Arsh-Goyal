class Solution {
public:
    int minMoves2(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int cnt = 0;
        for(int n:nums)
        {
            cnt++;
        }
        int m = nums[cnt/2];
        int ans = 0;
        for(int n:nums)
        {
            ans+=abs(n-m); 
        }
        return ans;
    }
};
